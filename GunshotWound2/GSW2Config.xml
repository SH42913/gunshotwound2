			<!-- Pistol;PistolMk2;CombatPistol;SNSPistol;SNSPistolMk2;VintagePistol;MarksmanPistol;DoubleActionRevolver;APPistol;MicroSMG;MiniSMG;SMG;SMGMk2;CombatPDW;MachinePistol;CeramicPistol;PericoPistol; -->
			<Hashes Hashes="453432689;3219281620;1593441988;3218215474;2285322324;137902532;3696079510;2548703416;584646201;324215364;3173288789;736523883;2024373456;171789620;3675956304;727643628;3514013252;"/>
		</SmallCaliber>

		<MediumCaliber DamageMult="1.2" BleedingMult="1.3" PainMult="1.5" CritChance="0.6" ArmorDamage="4">
			<!-- Pistol50;HeavyPistol;Revolver;RevolverMk2;AssaultRifleMk2;AdvancedRifle;AssaultSMG;BullpupRifle;BullpupRifleMk2;CarbineRifle;CarbineRifleMk2;CombatMG;CompactRifle;Gusenberg;SpecialCarbine;SpecialCarbineMk2;NavyRevolver;MilitaryRifle; -->
			<Hashes Hashes="2578377531;3523564046;3249783761;3415619887;961495388;2937143193;4024951519;2132975508;2228681469;2210333304;4208062921;2144741730;1649403952;1627465347;3231910285;2526821735;2441047180;2772307231;"/>
		</MediumCaliber>

		<HighCaliber DamageMult="1.3" BleedingMult="1.5" PainMult="1.8" CritChance="0.8" ArmorDamage="6">
			<!-- MG;CombatMGMk2;AssaultRifle;HeavySniper;HeavySniperMk2;MarksmanRifle;MarksmanRifleMk2;Minigun;Musket;SniperRifle;UnholyHellbringer; -->
			<Hashes Hashes="2634544996;3686625920;3220176749;205991906;177293209;3342088282;1785463520;1119849093;2828843422;100416529;1198256469;"/>
		</HighCaliber>

		<Shotgun DamageMult="0.8" BleedingMult="1.3" PainMult="1.5" CritChance="0.6" ArmorDamage="5">
			<!-- PumpShotgun;PumpShotgunMk2;SawnOffShotgun;BullpupShotgun;AssaultShotgun;HeavyShotgun;DoubleBarrelShotgun;SweeperShotgun;CombatShotgun; -->
			<Hashes Hashes="487013001;1432025498;2017895192;2640438543;3800352039;984333226;4019527611;317205821;3514615361;"/>
		</Shotgun>

		<LightImpact DamageMult="1.0" BleedingMult="1.0" PainMult="1.0" CritChance="0.1">
			<!-- Fist;Snowball;Nightstick;KnuckleDuster;Flashlight;Baseball;WaterCannon;Briefcase;Briefcase2;Parachute;NightVision; -->
			<Hashes Hashes="2725352035;126349499;1737195953;3638508604;2343591895;600439132;3425972830;2294779575;28811031;4222310262;2803906140;"/>
		</LightImpact>

		<HeavyImpact DamageMult="1.0" BleedingMult="1.0" PainMult="1.1" CritChance="0.7">
			<!-- Boar;Pig;Hammer;Crowbar;Bat;FireExtinguisher;GolfClub;PetrolCan;PoolCue;Wrench;Cow;Deer;StoneHatchet; -->
			<Hashes Hashes="861723357;1205296881;1317494643;2227010557;2508868239;101631238;1141786504;883325847;2484171525;419712736;94548753;4106648222;940833800;"/>
		</HeavyImpact>

		<Cutting DamageMult="1.0" BleedingMult="1.0" PainMult="1.0" CritChance="0.5">
			<!-- Knife;BattleAxe;Bottle;Dagger;Hatchet;Machete;SwitchBlade;Animal;Cougar;BarbedWire; -->
			<Hashes Hashes="2578778090;3441901897;4192643659;2460120199;4191993645;3713923289;3756226112;4194021054;148160082;1223143800;"/>
		</Cutting>

		<Ignore>
			<!-- GSW2 will ignore these weapons, so it will deal vanilla damage -->
			<!-- Molotov;BZGas;SmokeGrenade;StunGun;FlareGun;Flare;Up-n-Atomizer;GrenadeLauncherSmoke;FireWork;Railgun;RayMinigun;ElectricFence;Bleeding;Drowning;DrowningVehicle; -->
			<Hashes Hashes="615608432;2694266206;4256991824;911657153;1198879012;1233104067;2939590305;1305664598;2138347493;1834241177;3056410471;2461879995;2339582971;4284007675;1936677264;"/>
		</Ignore>
	</Weapons>

	<Hotkeys>
		<!-- If you don't want to use key, just remove key value
         KeyCodes can be found here - https://www.oreilly.com/library/view/javascript-dhtml/9780596514082/apb.html -->

		<GetHelmetKey>74</GetHelmetKey>
		<!-- Give to player motorbike helmet if the player has $40(can be changed in HelmetCost section).
        Helmet can save your head from impact and weapons.
        If the player already has a helmet, this hotkey will remove it.
        Default: J on keyboard. -->

		<BandageKey>75</BandageKey>
		<!-- Applied bandages to you. It will cost $15(can be changed in BandageCost).
		When Shift is pressed - will bandage the closest ped.
        Default: K on keyboard. -->

		<CheckKey>76</CheckKey>
		<!-- Check Yourself. Show one time status-notifications about your wounds and health.
		When Shift is pressed - will bandage the closest ped.
        Default: L on keyboard. -->

		<IncreaseRangeKey>33</IncreaseRangeKey>
		<!-- Increase GSW2 scan range.
        Default: PageUp on keyboard. -->

		<DecreaseRangeKey>34</DecreaseRangeKey>
		<!-- Decrease GSW2 scan range.
        Default: PageDown on keyboard. -->

		<PauseKey>35</PauseKey>
		<!-- Pause of the script.
		It can be useful in some missions.
        Default: End on keyboard. -->

		<DeathKey>46</DeathKey>
		<!-- Instant death. Useful if you're stuck in a deadly situation and don't want to wait for slow death.
		Default: Delete on keyboard. -->

		<!-- <HealKey></HealKey> -->
		<!-- Besides, you can set a key for instant heal -->
	</Hotkeys>
</GunshotWoundMod>