		</LightImpact>

		<HeavyImpact DamageMult="1.0" BleedingMult="1.0" PainMult="1.1" CritChance="0.7">
			<!-- Boar;Pig;Hammer;Crowbar;Bat;RunOverCar;FireExtinguisher;GolfClub;PetrolCan;PoolCue;Wrench;Cow;Deer;StoneHatchet; -->
